@using E_Commerce_App.Models;
@model List<Category>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

 @foreach (var item in Model)
    {

        <a asp-controller="Home" asp-action="Index" asp-route-cartID=@item.Id asp-route-userID=@item.Name>
            @item.ID @item.Name 
        </a>
   
    }